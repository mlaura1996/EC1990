###  http://qudt.org/3.1.2/schema/qudt/quantityValue
<http://qudt.org/3.1.2/schema/qudt/quantityValue> rdf:type owl:ObjectProperty .


###  http://www.w3id.org/eurocodes/ec1990#appliesTo
:appliesTo rdf:type owl:ObjectProperty ;
           rdfs:domain :Action ;
           rdfs:range :StructuralMember ;
           rdfs:comment "Relates an action to the structural member or structure it acts upon."@en ;
           rdfs:label "applies to"@en ;
           <http://www.w3.org/2004/02/skos/core#definition> "Relates an action to the structural member or structure it acts upon." .


###  http://www.w3id.org/eurocodes/ec1990#causesEffect
:causesEffect rdf:type owl:ObjectProperty ;
              rdfs:domain :CombinationOfActions ;
              rdfs:range :EffectOfAction ;
              rdfs:comment "Relates a combination of actions to the effects it causes in the structure."@en ;
              rdfs:label "causes effect"@en ;
              <http://www.w3.org/2004/02/skos/core#definition> "Relates a combination of actions to the effects it causes in the structure." .


###  http://www.w3id.org/eurocodes/ec1990#characteristicValue
:characteristicValue rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf <http://qudt.org/3.1.2/schema/qudt/quantityValue> ;
                     rdfs:domain <http://qudt.org/3.1.2/schema/qudt/Quantity> ;
                     rdfs:range :CharacteristicValue ;
                     rdfs:comment "Relates a Quantity  with its Characteristic Value."@en ;
                     rdfs:label "characteristic value"@en ;
                     <http://www.w3.org/2004/02/skos/core#definition> "Relates a Quantity  with its Characteristic Value." .


###  http://www.w3id.org/eurocodes/ec1990#containsAction
:containsAction rdf:type owl:ObjectProperty ;
                rdfs:domain :CombinationOfActions ;
                rdfs:range :Action ;
                rdfs:comment "Relates a combination of actions to the individual actions it contains."@en ;
                rdfs:label "contains action"@en ;
                <http://www.w3.org/2004/02/skos/core#definition> "Relates a combination of actions to the individual actions it contains." .


###  http://www.w3id.org/eurocodes/ec1990#containsStructuralMember
:containsStructuralMember rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf <https://w3id.org/bot#hasElement> ;
                          rdfs:domain :StructuralSystem ;
                          rdfs:range :StructuralMember ;
                          rdfs:comment "Relates a a structural element with the structural syste."@en ;
                          rdfs:label "contains structural member"@en ;
                          <http://www.w3.org/2004/02/skos/core#definition> "Relates a a structural element with the structural syste." .


###  http://www.w3id.org/eurocodes/ec1990#designValue
:designValue rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf <http://qudt.org/3.1.2/schema/qudt/quantityValue> ;
             rdfs:domain <http://qudt.org/3.1.2/schema/qudt/Quantity> ;
             rdfs:range :DesignValue ;
             rdfs:comment "Relates a Quantity  with its Design Value."@en ;
             rdfs:label "design value"@en ;
             <http://www.w3.org/2004/02/skos/core#definition> "Relates a Quantity  with its Design Value." .


###  http://www.w3id.org/eurocodes/ec1990#forLimitState
:forLimitState rdf:type owl:ObjectProperty ;
               rdfs:domain :LimitStateValue ;
               rdfs:range :LimitState ;
               rdfs:comment "Relates a limit state value with its corresponding limit state."@en ;
               rdfs:label "for limit state"@en ;
               <http://www.w3.org/2004/02/skos/core#definition> "Relates a limit state value with its corresponding limit state." .


###  http://www.w3id.org/eurocodes/ec1990#hasEffect
:hasEffect rdf:type owl:ObjectProperty ;
           rdfs:domain :StructuralMember ;
           rdfs:range :EffectOfAction ;
           rdfs:comment "Relates a structural member to the effect of action."@en ;
           rdfs:label "has effect"@en ;
           <http://www.w3.org/2004/02/skos/core#definition> "Relates a structural member to the effect of action." .


###  http://www.w3id.org/eurocodes/ec1990#hasGeometricalProperty
:hasGeometricalProperty rdf:type owl:ObjectProperty ;
                        rdfs:domain :StructuralMember ;
                        rdfs:range :GeometricalProperty ;
                        rdfs:comment "Relates a structural member with its geometrical properties."@en ;
                        rdfs:label "has geometrical property"@en ;
                        <http://www.w3.org/2004/02/skos/core#definition> "Relates a structural member with its geometrical properties." .


###  http://www.w3id.org/eurocodes/ec1990#hasLimitStateValue
:hasLimitStateValue rdf:type owl:ObjectProperty ;
                    rdfs:domain :StructuralMember ;
                    rdfs:range :LimitStateValue ;
                    rdfs:comment "Relates a structural member to its capacity for the effect of action."@en ;
                    rdfs:label "has limit state value"@en ;
                    <http://www.w3.org/2004/02/skos/core#definition> "Relates a structural member to its capacity for the effect of action." .


###  http://www.w3id.org/eurocodes/ec1990#hasMaterialProperty
:hasMaterialProperty rdf:type owl:ObjectProperty ;
                     rdfs:domain :Material ;
                     rdfs:range :MaterialProperty ;
                     rdfs:comment "Relates a material with its properties."@en ;
                     rdfs:label "has Property"@en ;
                     <http://www.w3.org/2004/02/skos/core#definition> "Relates a material with its properties." .


###  http://www.w3id.org/eurocodes/ec1990#hasStructuralMember
:hasStructuralMember rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf <https://w3id.org/bot#hasElement> ;
                     rdfs:domain <https://w3id.org/bot#Zone> ;
                     rdfs:range :StructuralMember ;
                     rdfs:comment "Relates a a structural element with the zone."@en ;
                     rdfs:label "has structural member"@en ;
                     <http://www.w3.org/2004/02/skos/core#definition> "Relates a a structural element with the zone." .


###  http://www.w3id.org/eurocodes/ec1990#hasStructure
:hasStructure rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <https://w3id.org/bot#hasElement> ;
              rdfs:domain :ConstructionWork ;
              rdfs:range :Structure ;
              rdfs:comment "Relates a contrution work with the structure."@en ;
              rdfs:label "has structure"@en ;
              <http://www.w3.org/2004/02/skos/core#definition> "Relates a contrution work with the structure." .


###  http://www.w3id.org/eurocodes/ec1990#hasSystem
:hasSystem rdf:type owl:ObjectProperty ;
           rdfs:domain :Structure ;
           rdfs:range :StructuralSystem ;
           rdfs:comment "Relates a structure with its structural system."@en ;
           rdfs:label "has system"@en ;
           <http://www.w3.org/2004/02/skos/core#definition> "Relates a structure with its structural system." .


###  http://www.w3id.org/eurocodes/ec1990#imposesCombination
:imposesCombination rdf:type owl:ObjectProperty ;
                    rdfs:domain :DesignSituation ;
                    rdfs:range :CombinationOfActions ;
                    rdfs:comment "Relates a design situation to the relevant combination."@en ;
                    rdfs:label "imposes combination"@en ;
                    <http://www.w3.org/2004/02/skos/core#definition> "Relates a design situation to the relevant combination." .


###  http://www.w3id.org/eurocodes/ec1990#isDesignedFor
:isDesignedFor rdf:type owl:ObjectProperty ;
               rdfs:domain :StructuralMember ;
               rdfs:range :DesignSituation ;
               rdfs:comment "Relates a structural memeber with the design situation it is designed for."@en ;
               rdfs:label "is designed for"@en ;
               <http://www.w3.org/2004/02/skos/core#definition> "Relates a structural memeber with the design situation it is designed for." .


###  http://www.w3id.org/eurocodes/ec1990#isMadeOf
:isMadeOf rdf:type owl:ObjectProperty ;
          rdfs:domain :StructuralMember ;
          rdfs:range :Material ;
          rdfs:comment "Relates a structural member to the material which it is made of."@en ;
          rdfs:label "is made of"@en ;
          <http://www.w3.org/2004/02/skos/core#definition> "Relates a structural member to the material which it is made of." .


###  http://www.w3id.org/eurocodes/ec1990#lowerValue
:lowerValue rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf <http://qudt.org/3.1.2/schema/qudt/quantityValue> ;
            rdfs:domain <http://qudt.org/3.1.2/schema/qudt/Quantity> ;
            rdfs:range :LowerValue ;
            rdfs:comment "Relates a Quantity  with its Lower Value."@en ;
            rdfs:label "lower value"@en ;
            <http://www.w3.org/2004/02/skos/core#definition> "Relates a Quantity  with its Lower Value." .


###  http://www.w3id.org/eurocodes/ec1990#meanValue
:meanValue rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf <http://qudt.org/3.1.2/schema/qudt/quantityValue> ;
           rdfs:domain <http://qudt.org/3.1.2/schema/qudt/Quantity> ;
           rdfs:range :MeanValue ;
           rdfs:comment "Relates a Quantity  with its Mean Value."@en ;
           rdfs:label "mean value"@en ;
           <http://www.w3.org/2004/02/skos/core#definition> "Relates a Quantity  with its Mean Value." .


###  http://www.w3id.org/eurocodes/ec1990#nominalValue
:nominalValue rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://qudt.org/3.1.2/schema/qudt/quantityValue> ;
              rdfs:domain <http://qudt.org/3.1.2/schema/qudt/Quantity> ;
              rdfs:range :NominalValue ;
              rdfs:comment "Relates a Quantity  with its Nominal Value."@en ;
              rdfs:label "nominal value"@en ;
              <http://www.w3.org/2004/02/skos/core#definition> "Relates a Quantity  with its Nominal Value." .


###  http://www.w3id.org/eurocodes/ec1990#representativeValue
:representativeValue rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf <http://qudt.org/3.1.2/schema/qudt/quantityValue> ;
                     rdfs:domain <http://qudt.org/3.1.2/schema/qudt/Quantity> ;
                     rdfs:range :RepresentativeValue ;
                     rdfs:comment "Relates a Quantity  with its Representative Value."@en ;
                     rdfs:label "representative value"@en ;
                     <http://www.w3.org/2004/02/skos/core#definition> "Relates a Quantity  with its Representative Value." .


###  http://www.w3id.org/eurocodes/ec1990#requiresVerficationOf
:requiresVerficationOf rdf:type owl:ObjectProperty ;
                       rdfs:domain :DesignSituation ;
                       rdfs:range :LimitState ;
                       rdfs:comment "Relates a limit state to the combination of actions used for its verification."@en ;
                       rdfs:label "requires verification of"@en ;
                       <http://www.w3.org/2004/02/skos/core#definition> "Relates a limit state to the combination of actions used for its verification." .


###  http://www.w3id.org/eurocodes/ec1990#upperValue
:upperValue rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf <http://qudt.org/3.1.2/schema/qudt/quantityValue> ;
            rdfs:domain <http://qudt.org/3.1.2/schema/qudt/Quantity> ;
            rdfs:range :UpperValue ;
            rdfs:comment "Relates a Quantity  with its Upper Value."@en ;
            rdfs:label "upper value"@en ;
            <http://www.w3.org/2004/02/skos/core#definition> "Relates a Quantity  with its Upper Value." .


###  http://www.w3id.org/eurocodes/ec1990#verifiedAgainstEffect
:verifiedAgainstEffect rdf:type owl:ObjectProperty ;
                       rdfs:domain :LimitStateValue ;
                       rdfs:range :EffectOfAction ;
                       rdfs:comment "Relates a limitstate value with the Action Effect that needs to be verified against."@en ;
                       rdfs:label "has limit state value"@en ;
                       <http://www.w3.org/2004/02/skos/core#definition> "Relates a limitstate value with the Action Effect that needs to be verified against." .


###  http://www.w3id.org/eurocodes/ec1990#verifiedFor
:verifiedFor rdf:type owl:ObjectProperty ;
             rdfs:domain :LimitState ;
             rdfs:range :CombinationOfActions ;
             rdfs:comment "Relates a limit state to the combination of actions used for its verification."@en ;
             rdfs:label "verified for"@en ;
             <http://www.w3.org/2004/02/skos/core#definition> "Relates a limit state to the combination of actions used for its verification." .


#################################################################
#    Data properties
#################################################################

###  http://www.w3id.org/eurocodes/ec1990#combinationFactor
:combinationFactor rdf:type owl:DatatypeProperty ;
                   rdfs:range xsd:decimal ;
                   <http://purl.org/dc/terms/source> "EN 1990:2002, Section 1.5.3.16" ;
                   rdfs:comment "Factor for combination value of a variable action used in ultimate limit state verifications."@en ;
                   rdfs:label "has combination factor"@en ;
                   <http://www.w3.org/2004/02/skos/core#altLabel> "ψ_0" ;
                   <http://www.w3.org/2004/02/skos/core#definition> "Factor for combination value of a variable action used in ultimate limit state verifications." .


###  http://www.w3id.org/eurocodes/ec1990#hasDesignWorkingLife
:hasDesignWorkingLife rdf:type owl:DatatypeProperty ;
                      rdfs:domain :Structure ;
                      rdfs:range xsd:decimal ;
                      rdfs:comment "Period during which a structure or structural component is intended to remain functional and to fulfill its performance requirements without major repair or replacement, assuming appropriate maintenance."@en ;
                      rdfs:label "has design working life"@en ;
                      <http://www.w3.org/2004/02/skos/core#definition> "Period during which a structure or structural component is intended to remain functional and to fulfill its performance requirements without major repair or replacement, assuming appropriate maintenance." .


###  http://www.w3id.org/eurocodes/ec1990#hasFrequentFactor
:hasFrequentFactor rdf:type owl:DatatypeProperty ;
                   rdfs:range xsd:decimal ;
                   <http://purl.org/dc/terms/source> "EN 1990:2002, Section 1.5.3.17" ;
                   rdfs:comment "Factor for frequent value of a variable action, determined so that either the total time within the reference period during which it is exceeded is only a small given part of the reference period, or the frequency of it being exceeded is limited to a given value."@en ;
                   rdfs:label "has frequent factor"@en ;
                   <http://www.w3.org/2004/02/skos/core#altLabel> "ψ_1" ;
                   <http://www.w3.org/2004/02/skos/core#definition> "Factor for frequent value of a variable action, determined so that either the total time within the reference period during which it is exceeded is only a small given part of the reference period, or the frequency of it being exceeded is limited to a given value." .


###  http://www.w3id.org/eurocodes/ec1990#hasPartialFactor
:hasPartialFactor rdf:type owl:DatatypeProperty ;
                  rdfs:range xsd:decimal ;
                  <http://purl.org/dc/terms/source> "EN 1990:2002, Section 6.3.1" ;
                  rdfs:comment "Safety factor applied to actions or material properties to account for uncertainties."@en ;
                  rdfs:label "Partial Factor"@en ;
                  <http://www.w3.org/2004/02/skos/core#altLabel> "γ_f" ;
                  <http://www.w3.org/2004/02/skos/core#definition> "Safety factor applied to actions or material properties to account for uncertainties." .


###  http://www.w3id.org/eurocodes/ec1990#hasQuasiPermanentFactor
:hasQuasiPermanentFactor rdf:type owl:DatatypeProperty ;
                         rdfs:range xsd:decimal ;
                         <http://purl.org/dc/terms/source> "EN 1990:2002, Section 1.5.3.18" ;
                         rdfs:comment "Factor for quasi-permanent value of a variable action, determined so that the total period of time for which it will be exceeded is a large fraction of the reference period."@en ;
                         rdfs:label "has quasi-permanent factor"@en ;
                         <http://www.w3.org/2004/02/skos/core#altLabel> "ψ_2" ;
                         <http://www.w3.org/2004/02/skos/core#definition> "Factor for quasi-permanent value of a variable action, determined so that the total period of time for which it will be exceeded is a large fraction of the reference period." .


###  http://www.w3id.org/eurocodes/ec1990#isSatisfied
:isSatisfied rdf:type owl:DatatypeProperty ;
             rdfs:domain :LimitState ;
             rdfs:range xsd:boolean ;
             rdfs:comment "Result of verification of the limit state."@en ;
             rdfs:label "is satisfied"@en ;
             <http://www.w3.org/2004/02/skos/core#definition> "Result of verification of the limit state." .


###  http://www.w3id.org/eurocodes/ec1990#reductionFactor
:reductionFactor rdf:type owl:DatatypeProperty ;
                 rdfs:range xsd:decimal ;
                 <http://purl.org/dc/terms/source> "EN 1990:2002, Section 6.4.3.2" ;
                 rdfs:comment "Reduction factor for unfavourable permanent action."@en ;
                 rdfs:label "has reduction factor"@en ;
                 <http://www.w3.org/2004/02/skos/core#altLabel> "ξ" ;
                 <http://www.w3.org/2004/02/skos/core#definition> "Reduction factor for unfavourable permanent action." .


#################################################################
#    Classes
#################################################################

###  http://qudt.org/3.1.2/schema/qudt/Quantity
<http://qudt.org/3.1.2/schema/qudt/Quantity> rdf:type owl:Class .


###  http://qudt.org/3.1.2/schema/qudt/QuantityValue
<http://qudt.org/3.1.2/schema/qudt/QuantityValue> rdf:type owl:Class .


###  http://www.w3id.org/eurocodes/ec1990#AccelerationEffect
:AccelerationEffect rdf:type owl:Class ;
                    rdfs:subClassOf :DynamicEffect ;
                    <http://purl.org/dc/terms/source> "EN 1990:2002, Section 4.1.5(2)" ;
                    rdfs:comment "Effect involving acceleration of the structure or structural members."@en ;
                    rdfs:label "Acceleration Effect"@en ;
                    <http://www.w3.org/2004/02/skos/core#definition> "Effect involving acceleration of the structure or structural members." .


###  http://www.w3id.org/eurocodes/ec1990#AccessibleRoofs
:AccessibleRoofs rdf:type owl:Class ;
                 rdfs:subClassOf :EurocodeZone ;
                 <http://purl.org/dc/terms/source> "EN 1991-1-1:2002 Table 6.9" ;
                 rdfs:comment "Roof accessible with occupancy according to categories A to G"@en ;
                 rdfs:label "Category I - Accessible Roofs"@en ;
                 <http://www.w3.org/2004/02/skos/core#definition> "Roof accessible with occupancy according to categories A to G" ;
                 <http://www.w3.org/2004/02/skos/core#example> "Roof used as terraces, gardens, or other occupied spaces" .


###  http://www.w3id.org/eurocodes/ec1990#AccidentalAction
:AccidentalAction rdf:type owl:Class ;
                  rdfs:subClassOf :Action ;
                  <http://purl.org/dc/terms/source> "EN 1990:2002, Section 1.5.3.5" ;
                  rdfs:comment "Action, usually of short duration but of significant magnitude, that is unlikely to occur on a given structure during the design working life."@en ;
                  rdfs:label "Accidental Action"@en ;
                  <http://www.w3.org/2004/02/skos/core#altLabel> "A" ;
                  <http://www.w3.org/2004/02/skos/core#definition> "Action, usually of short duration but of significant magnitude, that is unlikely to occur on a given structure during the design working life." ;
                  <http://www.w3.org/2004/02/skos/core#example> "explosions, impact from vehicles" .


###  http://www.w3id.org/eurocodes/ec1990#AccidentalCombination
:AccidentalCombination rdf:type owl:Class ;
                       rdfs:subClassOf :ULSCombination ;
                       owl:disjointWith :FundamentalCombination ,
                                        :SeismicCombination ;
                       <http://purl.org/dc/terms/source> "EN 1990:2002, Section 6.4.3.3" ;
                       rdfs:comment "Combination of actions for accidental design situations."@en ;
                       rdfs:label "Accidental Combination"@en ;
                       <http://www.w3.org/2004/02/skos/core#definition> "Combination of actions for accidental design situations." ;
                       <http://www.w3.org/2004/02/skos/core#scopeNote> "Used for accidental design situations in ULS verifications" .


###  http://www.w3id.org/eurocodes/ec1990#AccidentalDesignSituation
:AccidentalDesignSituation rdf:type owl:Class ;
                           rdfs:subClassOf :DesignSituation ;
                           <http://purl.org/dc/terms/source> "EN 1990:2002, Section 1.5.2.5" ;
                           rdfs:comment "Design situation involving exceptional conditions of the structure or its exposure, including fire, explosion, impact or local failure."@en ;
