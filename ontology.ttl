                                         <http://purl.org/dc/terms/creator> "Agnieszka Jędrzejewska (agnieszka.jedrzejewska@polsl.pl), Maria Laura Leonardi (mlauraleonardi@gmail.com), Carlos Ramonell Cazador (carlos.ramonell@upc.edu)" ;
                                         <http://purl.org/dc/terms/date> "2025-06-30"^^xsd:date ;
                                         <http://purl.org/dc/terms/description> "Core ontology formalising knowledge from Eurocode 0 for automated structural design and verification" ;
                                         <http://purl.org/dc/terms/format> "ttl" ;
                                         <http://purl.org/dc/terms/language> "en" ;
                                         <http://purl.org/dc/terms/modified> "2025-06-30"^^xsd:date ;
                                         <http://purl.org/dc/terms/title> "ECO - Eurocode Core Ontology" ;
