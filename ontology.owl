        <terms:creator>Agnieszka Jędrzejewska (agnieszka.jedrzejewska@polsl.pl), Maria Laura Leonardi (mlauraleonardi@gmail.com), Carlos Ramonell Cazador (carlos.ramonell@upc.edu)</terms:creator>
        <terms:date rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-06-30</terms:date>
        <terms:description>Core ontology formalising knowledge from Eurocode 0 for automated structural design and verification</terms:description>
        <terms:format>ttl</terms:format>
        <terms:language>en</terms:language>
