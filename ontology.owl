     -->


    


    <!-- http://qudt.org/3.1.2/schema/qudt/quantityValue -->


    <owl:ObjectProperty rdf:about="http://qudt.org/3.1.2/schema/qudt/quantityValue"/>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#appliesTo -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#appliesTo">
        <rdfs:domain rdf:resource="http://www.w3id.org/eurocodes/ec1990#Action"/>
        <rdfs:range rdf:resource="http://www.w3id.org/eurocodes/ec1990#StructuralMember"/>
        <rdfs:label xml:lang="en">applies to</rdfs:label>
        <skos:definition>Relates an action to the structural member or structure it acts upon.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#causesEffect -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#causesEffect">
        <rdfs:domain rdf:resource="http://www.w3id.org/eurocodes/ec1990#CombinationOfActions"/>
        <rdfs:range rdf:resource="http://www.w3id.org/eurocodes/ec1990#EffectOfAction"/>
        <rdfs:label xml:lang="en">causes effect</rdfs:label>
        <skos:definition>Relates a combination of actions to the effects it causes in the structure.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#containsStructuralMember -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#containsStructuralMember">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/bot#hasElement"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/eurocodes/ec1990#StructuralSystem"/>
        <rdfs:range rdf:resource="http://www.w3id.org/eurocodes/ec1990#StructuralMember"/>
        <rdfs:label xml:lang="en">contains structural member</rdfs:label>
        <skos:definition>Relates a a structural element with the structural system.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#forLimitState -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#forLimitState">
        <rdfs:domain rdf:resource="http://www.w3id.org/eurocodes/ec1990#LimitStateValue"/>
        <rdfs:range rdf:resource="http://www.w3id.org/eurocodes/ec1990#LimitState"/>
        <rdfs:label xml:lang="en">for limit state</rdfs:label>
        <skos:definition>Relates a limit state value with its corresponding limit state.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#hasCharacteristicValue -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#hasCharacteristicValue">
        <rdfs:subPropertyOf rdf:resource="http://qudt.org/3.1.2/schema/qudt/quantityValue"/>
        <rdfs:domain rdf:resource="http://qudt.org/3.1.2/schema/qudt/Quantity"/>
        <rdfs:range rdf:resource="http://www.w3id.org/eurocodes/ec1990#CharacteristicValue"/>
        <rdfs:label xml:lang="en">has characteristic value</rdfs:label>
        <skos:definition>Relates a Quantity  with its Characteristic Value.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#hasDesignValue -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#hasDesignValue">
        <rdfs:subPropertyOf rdf:resource="http://qudt.org/3.1.2/schema/qudt/quantityValue"/>
        <rdfs:domain rdf:resource="http://qudt.org/3.1.2/schema/qudt/Quantity"/>
        <rdfs:range rdf:resource="http://www.w3id.org/eurocodes/ec1990#DesignValue"/>
        <rdfs:label xml:lang="en">has design value</rdfs:label>
        <skos:definition>Relates a Quantity  with its Design Value.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#hasEffect -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#hasEffect">
        <rdfs:domain rdf:resource="http://www.w3id.org/eurocodes/ec1990#StructuralMember"/>
        <rdfs:range rdf:resource="http://www.w3id.org/eurocodes/ec1990#EffectOfAction"/>
        <rdfs:label xml:lang="en">has effect</rdfs:label>
        <skos:definition>Relates a structural member to the effect of action.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#hasGeometricalProperty -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#hasGeometricalProperty">
        <rdfs:domain rdf:resource="http://www.w3id.org/eurocodes/ec1990#StructuralMember"/>
        <rdfs:range rdf:resource="http://www.w3id.org/eurocodes/ec1990#GeometricalProperty"/>
        <rdfs:label xml:lang="en">has geometrical property</rdfs:label>
        <skos:definition>Relates a structural member with its geometrical properties.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#hasLimitStateValue -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#hasLimitStateValue">
        <rdfs:domain rdf:resource="http://www.w3id.org/eurocodes/ec1990#StructuralMember"/>
        <rdfs:range rdf:resource="http://www.w3id.org/eurocodes/ec1990#LimitStateValue"/>
        <rdfs:label xml:lang="en">has limit state value</rdfs:label>
        <skos:definition>Relates a structural member to its capacity for the effect of action.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#hasLowerBoundValue -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#hasLowerBoundValue">
        <rdfs:subPropertyOf rdf:resource="http://qudt.org/3.1.2/schema/qudt/quantityValue"/>
        <rdfs:domain rdf:resource="http://qudt.org/3.1.2/schema/qudt/Quantity"/>
        <rdfs:range rdf:resource="http://www.w3id.org/eurocodes/ec1990#LowerBoundValue"/>
        <rdfs:label xml:lang="en">has lower bound value</rdfs:label>
        <skos:definition>Relates a Quantity  with its Lower Bound Value.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#hasMaterialProperty -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#hasMaterialProperty">
        <rdfs:domain rdf:resource="http://www.w3id.org/eurocodes/ec1990#Material"/>
        <rdfs:range rdf:resource="http://www.w3id.org/eurocodes/ec1990#MaterialProperty"/>
        <rdfs:label xml:lang="en">has material property</rdfs:label>
        <skos:definition>Relates a material with its properties.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#hasMeanValue -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#hasMeanValue">
        <rdfs:subPropertyOf rdf:resource="http://qudt.org/3.1.2/schema/qudt/quantityValue"/>
        <rdfs:domain rdf:resource="http://qudt.org/3.1.2/schema/qudt/Quantity"/>
        <rdfs:range rdf:resource="http://www.w3id.org/eurocodes/ec1990#MeanValue"/>
        <rdfs:label xml:lang="en">has mean value</rdfs:label>
        <skos:definition>Relates a Quantity  with its Mean Value.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#hasNominalValue -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#hasNominalValue">
        <rdfs:subPropertyOf rdf:resource="http://qudt.org/3.1.2/schema/qudt/quantityValue"/>
        <rdfs:domain rdf:resource="http://qudt.org/3.1.2/schema/qudt/Quantity"/>
        <rdfs:range rdf:resource="http://www.w3id.org/eurocodes/ec1990#NominalValue"/>
        <rdfs:label xml:lang="en">has nominal value</rdfs:label>
        <skos:definition>Relates a Quantity  with its Nominal Value.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#hasRepresentativeValue -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#hasRepresentativeValue">
        <rdfs:subPropertyOf rdf:resource="http://qudt.org/3.1.2/schema/qudt/quantityValue"/>
        <rdfs:domain rdf:resource="http://qudt.org/3.1.2/schema/qudt/Quantity"/>
        <rdfs:range rdf:resource="http://www.w3id.org/eurocodes/ec1990#RepresentativeValue"/>
        <rdfs:label xml:lang="en">has representative value</rdfs:label>
        <skos:definition>Relates a Quantity  with its Representative Value.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#hasStructuralMember -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#hasStructuralMember">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/bot#hasElement"/>
        <rdfs:domain rdf:resource="https://w3id.org/bot#Zone"/>
        <rdfs:range rdf:resource="http://www.w3id.org/eurocodes/ec1990#StructuralMember"/>
        <rdfs:label xml:lang="en">has structural member</rdfs:label>
        <skos:definition>Relates a a structural element with the zone.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#hasStructure -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#hasStructure">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/bot#hasElement"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/eurocodes/ec1990#ConstructionWork"/>
        <rdfs:range rdf:resource="http://www.w3id.org/eurocodes/ec1990#Structure"/>
        <rdfs:label xml:lang="en">has structure</rdfs:label>
        <skos:definition>Relates a contrution work with the structure.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#hasSystem -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#hasSystem">
        <rdfs:domain rdf:resource="http://www.w3id.org/eurocodes/ec1990#Structure"/>
        <rdfs:range rdf:resource="http://www.w3id.org/eurocodes/ec1990#StructuralSystem"/>
        <rdfs:label xml:lang="en">has system</rdfs:label>
        <skos:definition>Relates a structure with its structural system.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#hasUpperBoundValue -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#hasUpperBoundValue">
        <rdfs:subPropertyOf rdf:resource="http://qudt.org/3.1.2/schema/qudt/quantityValue"/>
        <rdfs:domain rdf:resource="http://qudt.org/3.1.2/schema/qudt/Quantity"/>
        <rdfs:range rdf:resource="http://www.w3id.org/eurocodes/ec1990#UpperBoundValue"/>
        <rdfs:label xml:lang="en">has upper bound value</rdfs:label>
        <skos:definition>Relates a Quantity  with its Upper Bound Value.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#imposesCombination -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#imposesCombination">
        <rdfs:domain rdf:resource="http://www.w3id.org/eurocodes/ec1990#DesignSituation"/>
        <rdfs:range rdf:resource="http://www.w3id.org/eurocodes/ec1990#CombinationOfActions"/>
        <rdfs:label xml:lang="en">imposes combination</rdfs:label>
        <skos:definition>Relates a design situation to the relevant combination.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#isDesignedFor -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#isDesignedFor">
        <rdfs:domain rdf:resource="http://www.w3id.org/eurocodes/ec1990#StructuralMember"/>
        <rdfs:range rdf:resource="http://www.w3id.org/eurocodes/ec1990#DesignSituation"/>
        <rdfs:label xml:lang="en">is designed for</rdfs:label>
        <skos:definition>Relates a structural memeber with the design situation it is designed for.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#isMadeOf -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#isMadeOf">
        <rdfs:domain rdf:resource="http://www.w3id.org/eurocodes/ec1990#StructuralMember"/>
        <rdfs:range rdf:resource="http://www.w3id.org/eurocodes/ec1990#Material"/>
        <rdfs:label xml:lang="en">is made of</rdfs:label>
        <skos:definition>Relates a structural member to the material which it is made of.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#isVerifiedFor -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#isVerifiedFor">
        <rdfs:domain rdf:resource="http://www.w3id.org/eurocodes/ec1990#LimitState"/>
        <rdfs:range rdf:resource="http://www.w3id.org/eurocodes/ec1990#CombinationOfActions"/>
        <rdfs:label xml:lang="en">is verified for</rdfs:label>
        <skos:definition>Relates a limit state to the combination of actions used for its verification.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#requiresVerficationOf -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#requiresVerficationOf">
        <rdfs:domain rdf:resource="http://www.w3id.org/eurocodes/ec1990#DesignSituation"/>
        <rdfs:range rdf:resource="http://www.w3id.org/eurocodes/ec1990#LimitState"/>
        <rdfs:label xml:lang="en">requires verification of</rdfs:label>
        <skos:definition>Relates a limit state to the combination of actions used for its verification.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#usesActionValue -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#usesActionValue">
        <rdfs:domain rdf:resource="http://www.w3id.org/eurocodes/ec1990#CombinationOfActions"/>
        <rdfs:range rdf:resource="http://qudt.org/3.1.2/schema/qudt/QuantityValue"/>
        <rdfs:label xml:lang="en">uses action value</rdfs:label>
        <skos:definition>Relates a combination of actions to the individual actions it contains.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#verifiedAgainstEffect -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#verifiedAgainstEffect">
        <rdfs:domain rdf:resource="http://www.w3id.org/eurocodes/ec1990#LimitStateValue"/>
        <rdfs:range rdf:resource="http://www.w3id.org/eurocodes/ec1990#EffectOfAction"/>
        <rdfs:label xml:lang="en">has limit state value</rdfs:label>
        <skos:definition>Relates a limitstate value with the Action Effect that needs to be verified against.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3id.org/eurocodes/ec1990#hasCombinationFactor -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#hasCombinationFactor">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <terms:source>EN 1990:2002, Section 1.5.3.16</terms:source>
        <rdfs:label xml:lang="en">has combination factor</rdfs:label>
        <skos:altLabel>ψ_0</skos:altLabel>
        <skos:definition>Factor for combination value of a variable action used in ultimate limit state verifications.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#hasDesignWorkingLife -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#hasDesignWorkingLife">
        <rdfs:domain rdf:resource="http://www.w3id.org/eurocodes/ec1990#Structure"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:label xml:lang="en">has design working life</rdfs:label>
        <skos:definition>Period during which a structure or structural component is intended to remain functional and to fulfill its performance requirements without major repair or replacement, assuming appropriate maintenance.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#hasFrequentFactor -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#hasFrequentFactor">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <terms:source>EN 1990:2002, Section 1.5.3.17</terms:source>
        <rdfs:label xml:lang="en">has frequent factor</rdfs:label>
        <skos:altLabel>ψ_1</skos:altLabel>
        <skos:definition>Factor for frequent value of a variable action, determined so that either the total time within the reference period during which it is exceeded is only a small given part of the reference period, or the frequency of it being exceeded is limited to a given value.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#hasNationallyDefinedValue -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#hasNationallyDefinedValue">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:label xml:lang="en">has nationally defined value</rdfs:label>
        <skos:definition>Defines whether parameter has specific value defined in NA.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#hasPartialFactor -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#hasPartialFactor">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <terms:source>EN 1990:2002, Section 6.3.1</terms:source>
        <rdfs:label xml:lang="en">has partial factor</rdfs:label>
        <skos:altLabel>γ_f</skos:altLabel>
        <skos:definition>Safety factor applied to actions or material properties to account for uncertainties.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#hasQuasiPermanentFactor -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#hasQuasiPermanentFactor">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <terms:source>EN 1990:2002, Section 1.5.3.18</terms:source>
        <rdfs:label xml:lang="en">has quasi-permanent factor</rdfs:label>
        <skos:altLabel>ψ_2</skos:altLabel>
        <skos:definition>Factor for quasi-permanent value of a variable action, determined so that the total period of time for which it will be exceeded is a large fraction of the reference period.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#hasReductionFactor -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#hasReductionFactor">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <terms:source>EN 1990:2002, Section 6.4.3.2</terms:source>
        <rdfs:label xml:lang="en">has reduction factor</rdfs:label>
        <skos:altLabel>ξ</skos:altLabel>
        <skos:definition>Reduction factor for unfavourable permanent action.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/eurocodes/ec1990#isSatisfied -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/eurocodes/ec1990#isSatisfied">
        <rdfs:domain rdf:resource="http://www.w3id.org/eurocodes/ec1990#LimitState"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:label xml:lang="en">is satisfied</rdfs:label>
        <skos:definition>Result of verification of the limit state.</skos:definition>
    </owl:DatatypeProperty>
    


